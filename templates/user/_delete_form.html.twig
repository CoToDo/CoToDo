<form class="d-inline" method="post" action="{{ path('user_delete', {'id': userIn.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ userIn.id) }}">
    <button class="btn btn-outline-danger">Delete</button>
</form>