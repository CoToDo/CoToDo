            <div class="dropdown-divider"></div>
            <a class="dropdown-item" href="{{ path('user_show', {'id': app.user.id}) }}"><i class="fas fa-cog"></i>
                Settings</a>
            <div class="dropdown-divider"></div>
            <a class="dropdown-item" href="{{ path('logout') }}"><i class="fas fa-sign-out-alt"></i> Logout</a>
        </div>
    </div>

